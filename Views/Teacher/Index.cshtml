@model List<Teachers>



<table>
    <thead>
        <tr>
            <th>Teacher Name</th>
            <th>Teacher Age</th>
            <th>Teacher Course</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Teachers teachers in Model)
        {
            <tr>
                <td>@teachers.TeacherName</td>
                <td>@teachers.TeacherAge</td>
                <td>@teachers.TeacherCoures</td>
                <td><a href="/Teacher/Delete/@teachers.TeacherId">Delete</a></td>
            </tr>
        }
    </tbody>
</table>

<button>
    <a href="/Teacher/Create"> <span>Add Teacher</span>  </a>
</button>